    "@wdio/spec-reporter": "7.7.0",
    "allure-commandline": "2.13.8",
    "cucumber-messages": "^8.0.0",
    "cucumber-tag-expressions": "2.0.3",
    "chromedriver": "^108.0",
    "tsconfig-paths": "3.9.0",
